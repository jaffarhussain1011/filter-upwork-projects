{
  "name": "Filter upwork projects",
  "description": "The power of React for building interactive Chrome extensions",
  "author": "Jaffar Hussain <jaffarhussain1011@gmail.com>",
  "version": "1.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.upwork.com/*"
      ],
      "js": [
        "static/filter-projects.js"
      ]
    }
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "tabs"
] 
}